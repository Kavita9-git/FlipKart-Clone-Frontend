{
  "cli": {
    "version": ">= 6.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "image": "latest",
        "env": {
          "EXPO_USE_GRADLE_WRAPPER": "true"
        },
        "java": "17"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
